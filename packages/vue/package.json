{
  "name": "@forms.js/vue",
  "version": "1.0.13",
  "description": "The official vue component for Forms.js",
  "module": "lib/index.js",
  "type": "module",
  "types": "lib/index.d.ts",
  "files": [
    "lib/**/*"
  ],
  "scripts": {
    "build": "tsc  && tsc -p tsconfig-cjs.json && tsc -p tsconfig-local-testing.json",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "eslint src/** -c eslint.json",
    "patch": "npm run format && npm run lint && npm run build && npm version patch",
    "update": "npm run lint && npm update --save && npm run build"
  },
  "exports": {
    ".": {
      "import": "./lib/index.js",
      "require": "./lib/cjs/index.js"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/form-js/forms.js"
  },
  "keywords": [
    "vue forms",
    "forms vue",
    "vue form generator",
    "vue form builder"
  ],
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/form-js/forms.js/issues"
  },
  "homepage": "https://github.com/form-js",
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/quill": "^2.0.14",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "@vue/test-utils": "^2.4.6",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "jest": "^29.7.0",
    "prettier": "^3.2.5",
    "ts-jest": "^29.1.2",
    "typescript": "^5.4.5"
  },
  "dependencies": {
    "@forms.js/core": "^1.1.12",
    "install": "^0.13.0",
    "npm": "^10.8.0",
    "vue": "^3.4.18"
  }
}
